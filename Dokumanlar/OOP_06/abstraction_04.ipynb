    "from abc import ABC,abstractmethod\n",
    "class A(ABC):\n",
    "    @abstractmethod\n",
    "    def ornek(self):\n",
    "        pass\n",
    "\n",
    "class B(A):\n",
    "    def __init__(self):\n",
    "        self.b = \"B\"\n",
    "    def ornek(self):\n",
    "        print(self.b)\n",
    "n1 = B()"
